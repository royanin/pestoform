<script>
$(document).ready(function(){
    $("button-{{ item.id }}").click(function(){
        var muddy_like_count = $('#muddy-{{ item.like_count }}').val();
        $.ajax({
                url: "/update_vote",
                async: false,
                data: JSON.stringify( { "vote" : "1" } ),
                success: function(result){
                        $("#vote-{{ item.id }}").html(result);
                },
                error: function(error){
				    $("#vote-{{ item.id }}").log(error);
                }
        });
    });
});
</script>

